{
	"name": "git-add-commit-push",
	"publisher": "alfredbirk",
	"icon": "images/logo.png",
	"displayName": "Git add commit push",
	"description": "Three git commands in one easy shortcut!",
	"version": "0.8.0",
	"engines": {
		"vscode": "^1.16.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"git",
		"add",
		"commit",
		"push",
		"shortcut",
		"hotkey",
		"auto",
		"macro"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/alfredbirk/vscode-extension-git-add-commit-push"
	},
	"activationEvents": [
		"onCommand:gitAddCommitPush.gitAddCommitPush",
		"onCommand:gitAddCommitPush.gitAddCommitPushPr"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gitAddCommitPush.gitAddCommitPush",
				"title": "Git add > commit > push"
			},
			{
				"command": "gitAddCommitPush.gitAddCommitPushPr",
				"title": "Git add > commit > push > create PR"
			}
		],
		"keybindings": [
			{
				"command": "gitAddCommitPush.gitAddCommitPush",
				"key": "cmd+ctrl+p"
			},
			{
				"command": "gitAddCommitPush.gitAddCommitPushPr",
				"key": "cmd+ctrl+l"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.2.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
